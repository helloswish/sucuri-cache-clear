{
    "name": "helloswish/craft-sucuri-cache-clear",
    "description": "Purge Sucuri caches from Craft.",
    "type": "craft-plugin",
    "version": "5.0.2",
    "license": "mit",
    "support": {
        "email": "support@swishdigital.co",
        "issues": "https://github.com/helloswish/sucuri-cache-clear/issues?state=open",
        "source": "https://github.com/helloswish/sucuri-cache-clear",
        "docs": "https://github.com/helloswish/sucuri-cache-clear",
        "rss": "https://github.com/helloswish/sucuri-cache-clear/releases.atom"
    },
    "require": {
        "php": ">=8.0.2",
        "craftcms/cms": "^4.3.5|^5.0.x-dev|^5.0.0",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "helloswish\\craftsucuricacheclear\\": "src/"
        }
    },
    "extra": {
        "handle": "sucuri-cache-clear",
        "name": "Sucuri Cache Clear",
        "developer": "Swish Digital",
        "documentationUrl": "https://github.com/helloswish/sucuri-cache-clear",
        "class": "helloswish\\craftsucuricacheclear\\Sucuri"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}

